{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@drafto/core": {
      "projectType": "library",
      "root": "projects/drafto",
      "sourceRoot": "projects/drafto",
      "prefix": "dft",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/drafto/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/drafto/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/drafto/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/drafto/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"],
            "karmaConfig": "projects/drafto/karma.conf.js"
          }
        }
      }
    },
    "@drafto/material": {
      "projectType": "library",
      "root": "projects/drafto-material",
      "sourceRoot": "projects/drafto-material",
      "prefix": "dft-mat",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/drafto-material/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/drafto-material/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/drafto-material/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/drafto-material/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"],
            "karmaConfig": "projects/drafto-material/karma.conf.js"
          }
        }
      }
    },
    "webadmin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/webadmin",
      "sourceRoot": "projects/webadmin/src",
      "prefix": "dftwa",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/webadmin",
            "index": "projects/webadmin/src/index.html",
            "browser": "projects/webadmin/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/webadmin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/webadmin/public"
              }
            ],
            "styles": ["projects/webadmin/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kB",
                  "maximumError": "20kB"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "projects/webadmin/src/environments/environment.ts",
                  "with": "projects/webadmin/src/environments/environment.prod.ts"
                }
              ]
            },
            "beta": {
              "optimization": true,
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "projects/webadmin/src/environments/environment.ts",
                  "with": "projects/webadmin/src/environments/environment.beta.ts"
                }
              ]
            },
            "dev": {
              "optimization": true,
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "projects/webadmin/src/environments/environment.ts",
                  "with": "projects/webadmin/src/environments/environment.dev.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "webadmin:build:production"
            },
            "development": {
              "buildTarget": "webadmin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/webadmin/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/webadmin/public"
              }
            ],
            "styles": ["projects/webadmin/src/styles.scss"],
            "scripts": [],
            "karmaConfig": "projects/webadmin/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
